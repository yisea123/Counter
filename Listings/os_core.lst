S        y = OSUnMapTbl[ pevent->OSEventGrp & 0xFFu];
S    } else {
S        y = OSUnMapTbl[(OS_PRIO)(pevent->OSEventGrp >> 8u) & 0xFFu] + 8u;
S    }
S    ptbl = &pevent->OSEventTbl[y];
S    if ((*ptbl & 0xFFu) != 0u) {
