NINT8U  OS_TCBInit (INT8U    prio,
N                   OS_STK  *ptos,
N                   OS_STK  *pbos,
N                   INT16U   id,
N                   INT32U   stk_size,
N                   void    *pext,
N                   INT16U   opt)
