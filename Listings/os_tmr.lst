S#endif
S    if (ptmr->OSTmrType != OS_TMR_TYPE) {                         /* Validate timer structure                         */
S        *perr = OS_ERR_TMR_INVALID_TYPE;
S        return (OS_FALSE);
S    }
S    if (OSIntNesting > 0u) {                                      /* See if trying to call from an ISR                */
S        *perr  = OS_ERR_TMR_ISR;
S        return (OS_FALSE);
S    }
S    OSSchedLock();
S    switch (ptmr->OSTmrState) {
S        case OS_TMR_STATE_RUNNING:
S             OSTmr_Unlink(ptmr);                                  /* Remove from current wheel spoke                  */
S             *perr = OS_ERR_NONE;
S             switch (opt) {
S                 case OS_TMR_OPT_CALLBACK:
S                      pfnct = ptmr->OSTmrCallback;                /* Execute callback function if available ...       */
S                      if (pfnct != (OS_TMR_CALLBACK)0) {
S                          (*pfnct)((void *)ptmr, ptmr->OSTmrCallbackArg);  /* Use callback arg when timer was created */
S                      } else {
S                          *perr = OS_ERR_TMR_NO_CALLBACK;
S                      }
S                      break;
S
S                 case OS_TMR_OPT_CALLBACK_ARG:
S                      pfnct = ptmr->OSTmrCallback;                /* Execute callback function if available ...       */
S                      if (pfnct != (OS_TMR_CALLBACK)0) {
S                          (*pfnct)((void *)ptmr, callback_arg);   /* ... using the 'callback_arg' provided in call    */
S                      } else {
S                          *perr = OS_ERR_TMR_NO_CALLBACK;
S                      }
S                      break;
S
S                 case OS_TMR_OPT_NONE:
S                      break;
S
S                 default:
S                     *perr = OS_ERR_TMR_INVALID_OPT;
S                     break;
S             }
S             OSSchedUnlock();
S             return (OS_TRUE);
S
S        case OS_TMR_STATE_COMPLETED:                              /* Timer has already completed the ONE-SHOT or ...  */
S        case OS_TMR_STATE_STOPPED:                                /* ... timer has not started yet.                   */
S             OSSchedUnlock();
S             *perr = OS_ERR_TMR_STOPPED;
S             return (OS_TRUE);
S
S        case OS_TMR_STATE_UNUSED:                                 /* Timer was not created                            */
S             OSSchedUnlock();
S             *perr = OS_ERR_TMR_INACTIVE;
S             return (OS_FALSE);
S
S        default:
S             OSSchedUnlock();
S             *perr = OS_ERR_TMR_INVALID_STATE;
S             return (OS_FALSE);
S    }
S}
