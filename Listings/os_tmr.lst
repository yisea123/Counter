N/*
N************************************************************************************************************************
N*                                               ALLOCATE AND FREE A TIMER
N*
N* Description: This function is called to allocate a timer.
N*
N* Arguments  : none
N*
N* Returns    : a pointer to a timer if one is available
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0u
X#if 0u > 0u
Sstatic  OS_TMR  *OSTmr_Alloc (void)
S{
S    OS_TMR *ptmr;
S
S
S    if (OSTmrFreeList == (OS_TMR *)0) {
S        return ((OS_TMR *)0);
S    }
S    ptmr            = (OS_TMR *)OSTmrFreeList;
S    OSTmrFreeList   = (OS_TMR *)ptmr->OSTmrNext;
S    ptmr->OSTmrNext = (OS_TCB *)0;
S    ptmr->OSTmrPrev = (OS_TCB *)0;
S    OSTmrUsed++;
S    OSTmrFree--;
S    return (ptmr);
S}
N#endif
N
N
N/*
N************************************************************************************************************************
N*                                             RETURN A TIMER TO THE FREE LIST
N*
N* Description: This function is called to return a timer object to the free list of timers.
N*
N* Arguments  : ptmr     is a pointer to the timer to free
N*
N* Returns    : none
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0u
X#if 0u > 0u
Sstatic  void  OSTmr_Free (OS_TMR *ptmr)
S{
S    ptmr->OSTmrState       = OS_TMR_STATE_UNUSED;      /* Clear timer object fields                                   */
S    ptmr->OSTmrOpt         = OS_TMR_OPT_NONE;
S    ptmr->OSTmrPeriod      = 0u;
S    ptmr->OSTmrMatch       = 0u;
S    ptmr->OSTmrCallback    = (OS_TMR_CALLBACK)0;
S    ptmr->OSTmrCallbackArg = (void *)0;
S#if OS_TMR_CFG_NAME_EN > 0u
S    ptmr->OSTmrName        = (INT8U *)(void *)"?";
S#endif
S
S    ptmr->OSTmrPrev        = (OS_TCB *)0;              /* Chain timer to free list                                    */
S    ptmr->OSTmrNext        = OSTmrFreeList;
S    OSTmrFreeList          = ptmr;
S
S    OSTmrUsed--;                                       /* Update timer object statistics                              */
S    OSTmrFree++;
S}
N#endif
