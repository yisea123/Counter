N/*$PAGE*/
N/*
N************************************************************************************************************************
N*                                                    INITIALIZATION
N*                                          INITIALIZE THE FREE LIST OF TIMERS
N*
N* Description: This function is called by OSInit() to initialize the free list of OS_TMRs.
N*
N* Arguments  : none
N*
N* Returns    : none
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0u
X#if 0u > 0u
Svoid  OSTmr_Init (void)
S{
S#if OS_EVENT_NAME_EN > 0u
S    INT8U    err;
S#endif
S    INT16U   ix;
S    INT16U   ix_next;
S    OS_TMR  *ptmr1;
S    OS_TMR  *ptmr2;
S
S
S    OS_MemClr((INT8U *)&OSTmrTbl[0],      sizeof(OSTmrTbl));            /* Clear all the TMRs                         */
S    OS_MemClr((INT8U *)&OSTmrWheelTbl[0], sizeof(OSTmrWheelTbl));       /* Clear the timer wheel                      */
S
S    for (ix = 0u; ix < (OS_TMR_CFG_MAX - 1u); ix++) {                   /* Init. list of free TMRs                    */
S        ix_next = ix + 1u;
S        ptmr1 = &OSTmrTbl[ix];
S        ptmr2 = &OSTmrTbl[ix_next];
S        ptmr1->OSTmrType    = OS_TMR_TYPE;
S        ptmr1->OSTmrState   = OS_TMR_STATE_UNUSED;                      /* Indicate that timer is inactive            */
S        ptmr1->OSTmrNext    = (void *)ptmr2;                            /* Link to next timer                         */
S#if OS_TMR_CFG_NAME_EN > 0u
S        ptmr1->OSTmrName    = (INT8U *)(void *)"?";
S#endif
S    }
S    ptmr1               = &OSTmrTbl[ix];
S    ptmr1->OSTmrType    = OS_TMR_TYPE;
S    ptmr1->OSTmrState   = OS_TMR_STATE_UNUSED;                          /* Indicate that timer is inactive            */
S    ptmr1->OSTmrNext    = (void *)0;                                    /* Last OS_TMR                                */
S#if OS_TMR_CFG_NAME_EN > 0u
S    ptmr1->OSTmrName    = (INT8U *)(void *)"?";
S#endif
S    OSTmrTime           = 0u;
S    OSTmrUsed           = 0u;
S    OSTmrFree           = OS_TMR_CFG_MAX;
S    OSTmrFreeList       = &OSTmrTbl[0];
S    OSTmrSem            = OSSemCreate(1u);
S    OSTmrSemSignal      = OSSemCreate(0u);
S
S#if OS_EVENT_NAME_EN > 0u                                               /* Assign names to semaphores                 */
S    OSEventNameSet(OSTmrSem,       (INT8U *)(void *)"uC/OS-II TmrLock",   &err);
S    OSEventNameSet(OSTmrSemSignal, (INT8U *)(void *)"uC/OS-II TmrSignal", &err);
S#endif
S
S    OSTmr_InitTask();
S}
N#endif
